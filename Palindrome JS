function palindrome(str) {
  var start = str.toLowerCase().replace(/[^a-z0-9]/gmi, " ").replace(/\s+/g, " ").replace(/ /g,''); 
  var rev = start.split("").reverse().join("");
  //.replace(/ /g,'')
  console.log(rev);
  console.log(start);
  if (rev === start){
  return true;
  } else 
  return false
  
}
